mmerce.cart.remove(productId)\r\n            setCart(cart)\r\n        } catch (error) {\r\n            console.log(error, 'error from cart.remove()')\r\n        }\r\n    }\r\n\r\n    const handleEmptyCart = async () => {\r\n        try {\r\n            const {cart} = await commerce.cart.empty()\r\n            setCart(cart)\r\n        } catch (error) {\r\n            console.log(error, 'error from cart.empty()')\r\n        }\r\n    }\r\n\r\n    useEffect(() => {\r\n        fetchProducts()\r\n        fetchCart() \r\n    }, [])\r\n    return (\r\n        <Router>\r\n        <div className=\"main_container\">\r\n            <NavBar totalItems = {cart.total_items} />\r\n            <Switch>\r\n                <Route exact path=\"/\">\r\n                <Products products={products} handleAddToCart={handleAddToCart}/>\r\n                </Route>\r\n                <Route exact path=\"/cart\">\r\n                <Cart cart={cart}\r\n                handleEmptyCart={handleEmptyCart}\r\n                handleRemoveFromCart={handleRemoveFromCart}\r\n                handleUpdateCartQty={handleUpdateCartQty}\r\n                />\r\n                </Route>\r\n                <Route exact path=\"/checkout\">\r\n                    <Checkout cart={cart}/>\r\n                </Route>\r\n                </Switch>\r\n        </div>\r\n        </Router>\r\n    )\r\n}\r\n\r\nexport default App\r\n","G:\\Udemy-React-Course\\e-commerce-react-stripejs\\e-commerce-new\\src\\lib\\commerce.js",[],"G:\\Udemy-React-Course\\e-commerce-react-stripejs\\e-commerce-new\\src\\components\\index.js",[],"G:\\Udemy-React-Course\\e-commerce-react-stripejs\\e-commerce-new\\src\\components\\products\\Products.js",["86","87"],"import React from 'react'\r\nimport {Grid} from '@material-ui/core'\r\nimport Product from '../product/Product'\r\nimport img1 from '../../img/e-commerce-shoe-1.png'\r\nimport img2 from '../../img/e-commerce-shoe-2.png'\r\nimport useStyles from './stylesProducts'\r\n\r\n\r\nconst Pr